#include<stdio.h>  
int main(){    
int arr[3];
int i;
printf("the element of array \n");
for(i=0;i<3;i++)
{
scanf("%d\t",&arr[i]);
}
int j;
int flag;
printf("the prime num of array ");
for(i=0;i<3;i++);{
    flag=0;
    for(j=2;j<arr[i];j++)
 {
     if(arr[i]%j==0)
     {
         flag=1;
         break;
     }
      } 
      if(flag==0);
      for(i=2;i>=0;i--)
      printf("  %d\t",arr[i]);
}

return 0;  
 }    
